<script>

let prato;
let bebida;
let sobremesa;



    function escolherFrango(){
    document.getElementById("frango").style.borderColor = "green";
    document.getElementById("torre").style.borderColor = "white";
    prato = "frango yin-yang";
    }
    function escolher(){
        document.getElementById("frango").style.borderColor = "white";
    document.getElementById("torre").style.borderColor = "green";
    prato = "torre de batatas";
    }
    function escolherBebida(){
        document.getElementById("agua").style.borderColor = "green";
        document.getElementById("coca").style.borderColor = "white";
bebida = "agua";
    }
    function escolherCoca(){
        document.getElementById("coca").style.borderColor = "green";
        document.getElementById("agua").style.borderColor = "white";
bebida = "pepsi"
    }
    function escolherAssai(){
        document.getElementById("assai").style.borderColor = "green";
        document.getElementById("milk shake").style.borderColor = "white";
        sobremesa = "barca de assai"
    }
    function escolherMilk(){
        document.getElementById("milk shake").style.borderColor = "green";
        document.getElementById("assai").style.borderColor = "white";
        sobremesa = "milk shake";
    }

    function finalizarPedido(){
        let mensagem

        mensagem = "Ol√° felpzera desejaria pedir " 
        + prato + ", " 
        + bebida + " e um " 
        + sobremesa 
        + " obrigado e estou no aguardo"

        window.open("https://wa.me/+5544991284959?text=" + mensagem);
    }
</script>

